<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thor.dao.ListingDAO">
    <!-- 定义一个ResultMap来映射数据库列到ListingDO实体类的属性 -->
    <resultMap id="ListingResultMap" type="com.thor.entity.ListingDO">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="userId" column="user_id" jdbcType="BIGINT"/>
        <result property="productDetailId" column="product_detail_id" jdbcType="BIGINT"/>
        <result property="status" column="status" jdbcType="INTEGER"/>
        <result property="brand" column="brand" jdbcType="VARCHAR"/>
    </resultMap>
</mapper>
